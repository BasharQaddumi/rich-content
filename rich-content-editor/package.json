{
  "private": false,
  "name": "wix-rich-content-editor",
  "version": "1.0.0",
  "author": {
    "name": "Jonathan Bachman",
    "email": "jonathanb@wix.com"
  },
  "main": "dist/wix-rich-content-editor.js",
  "files": ["dist"],
  "scripts": {
    "clean": "rm -rf dist/",
    "start": "webpack-dev-server --open",
    "prebuild": "npm run clean",
    "build": "webpack --progress",
    "prewatch": "npm run clean",
    "watch": "webpack --watch --colors --progress",
    "release": "wnpm-release --no-shrinkwrap",
    "format": "prettier --list-different --write 'src/**/*.{js,jsx}'",
    "lint": "eslint 'src/**/*.{js,jsx}'",
    "lint:fix": "npm run lint -- --fix",
    "pretest": "npm run lint",
    "test": "jest --verbose"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-runtime": "^6.22.0",
    "classnames": "^2.2.5",
    "decorate-component-with-props": "^1.0.2",
    "draft-js": "^0.10.4",
    "draft-js-drag-n-drop-plugin": "^2.0.1",
    "draft-js-focus-plugin": "^2.0.1",
    "draft-js-inline-toolbar-plugin": "^2.0.1",
    "draft-js-linkify-plugin": "^2.0.1",
    "draft-js-plugins-editor": "^2.0.1",
    "draft-js-side-toolbar-plugin": "^2.0.1",
    "draft-js-static-toolbar-plugin": "^2.0.1",
    "immutable": "^3.8.1",
    "lodash": "^4.17.4",
    "react-player": "^0.25.3",
    "react-sortable-hoc": "^0.6.5",
    "url-loader": "^0.6.2",
    "wix-style-react": "^1.2.3",
    "wnpm-ci": "latest"
  },
  "peerDependencies": {
    "pro-gallery-renderer": "^1.0.59"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.7",
    "enzyme": "^2.9.1",
    "eslint": "^4.15.0",
    "eslint-config-wix": "^1.1.16",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-lodash": "^2.5.0",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-react": "^7.5.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "jest": "^21.2.1",
    "loader-utils": "^1.1.0",
    "node-sass": "^4.5.3",
    "prettier": "^1.9.2",
    "prop-types": "^15.5.10",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-svg-loader": "^1.1.1",
    "react-test-renderer": "^15.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0"
  },
  "eslintConfig": {
    "extends": "wix/react"
  },
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  }
}
